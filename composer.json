{
    "name": "omarlopesino/drupal-recipe-seo-analytics",
    "description": "Modules to make a Drupal site SEO-friendly",
    "type": "drupal-recipe",
    "authors": [
        {
            "name": "Omar Lopesino",
            "email": "omar.lopesino@metadrop.net"
        }
    ],
   "require": {
        "drupal/metatag": "^2.1",
        "drupal/pathauto": "^1.13",
        "drupal/redirect": "^1.10",
        "drupal/simple_sitemap": "^4.2",
        "drupal/schema_metatag": "^3.0",
        "drupal/google_tag": "^2.0",
        "drupal/datalayer": "^2.1",
        "drupal/datalayer_webform": "^1.0"
   },
   "extra": {
     "patches": {
        "omarlopesino/drupal-recipe-seo-analytics": {
            "Content forms are painfully slow if multiple sub-modules are enabled - https://www.drupal.org/project/metatag/issues/3057523": "https://www.drupal.org/files/issues/2022-07-14/3057523-85-metatag-async-click-trigger-accessibility.patch"
        }
     }
   }
